{
  "name": "express-base-sequelize",
  "version": "1.0.0",
  "description": "This is base express build on top of sequelize",
  "main": "main.js",
  "scripts": {
    "lint": "eslint --fix --ext .js .",
    "dev": "nodemon --exec babel-node ./src/bin/www.js",
    "start": "node ./dist/bin/www.js",
    "build": "babel ./src --out-dir dist --copy-files",
    "model": "sequelize-cli model:generate --name",
    "seed": "sequelize-cli seed:generate --name",
    "migrate:latest": "sequelize-cli db:migrate",
    "migrate:rollback": "sequelize-cli db:migrate:undo:all",
    "migrate:reset": "yarn migrate:rollback && yarn migrate:latest && yarn seed:run",
    "seed:run": "sequelize db:seed:all",
    "seed:down": "sequelize db:seed:undo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dangphu2412/Express-Base-Sequelize.git"
  },
  "keywords": [
    "express",
    "sequelize",
    "javascript"
  ],
  "author": "dangphu2412",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dangphu2412/Express-Base-Sequelize/issues"
  },
  "homepage": "https://github.com/dangphu2412/Express-Base-Sequelize#readme",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cloudinary": "^1.23.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.0",
    "faker": "^5.1.0",
    "fs": "^0.0.1-security",
    "http-status": "^1.4.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.10",
    "passport": "^0.4.1",
    "path": "^0.12.7",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.2.0",
    "speakingurl": "^14.0.1",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@babel/runtime": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.21.2",
    "nodemon": "^2.0.4",
    "sequelize-cli": "^6.0.0"
  }
}
